{
	"name": "@social-network/web-client",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "cross-env ENV_FILE=development vite",
		"build:prod": "tsc && cross-env ENV_FILE=production vite build",
		"build:stage": "tsc && cross-env ENV_FILE=stage vite build",
		"build:dev": "tsc && cross-env ENV_FILE=development vite build",
		"preview": "vite preview",
		"lint:ts": "eslint \"**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
		"lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0 --fix **/*.{ts,tsx}",
		"test": "vitest",
		"test:unit": "jest --verbose --config ./config/jest/jest.config.ts",
		"test:ui:ok": "npx loki approve",
		"test:ui:ci": "npx loki --requireReference --reactUri file:./storybook-static",
		"test:ui:report": "npm run test:ui:report:json && npm run test:ui:report:html",
		"test:ui:report:json": "node lib/scripts/reg-cli.generate-visual-json-report.cjs",
		"test:ui:report:html": "reg-cli --from .loki/reg-cli.report.json --report .loki/reg-cli.report.html",
		"create:page": "gvegas-cfc create -t ./lib/templates/AsyncTemplate -o ./src/pages",
		"create:ui": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/shared/UI/",
		"create:widget": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/widgets/",
		"create:feature": "gvegas-cfc create -t ./lib/templates/UITemplate -o ./src/features/",
		"sb": "storybook dev -p 6006 -c ./config/storybook",
		"sb:build": "storybook build -c ./config/storybook",
		"prepare": "husky install"
	},
	"dependencies": {
		"@hookform/resolvers": "^3.3.1",
		"axios": "^1.5.0",
		"clsx": "^2.0.0",
		"html5-qrcode": "^2.3.8",
		"husky": "^8.0.3",
		"i18next": "^23.5.1",
		"i18next-http-backend": "^2.2.2",
		"immer": "^10.0.2",
		"lodash.throttle": "^4.1.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.46.1",
		"react-i18next": "^13.3.0",
		"react-router-dom": "^6.16.0",
		"react-transition-group": "^4.4.5",
		"swr": "^2.2.2",
		"uuid": "^9.0.1",
		"zod": "^3.22.2",
		"zustand": "^4.4.1"
	},
	"devDependencies": {
		"@storybook/addon-essentials": "^7.4.2",
		"@storybook/addon-interactions": "^7.4.2",
		"@storybook/addon-links": "^7.4.2",
		"@storybook/addon-onboarding": "^1.0.8",
		"@storybook/blocks": "^7.4.2",
		"@storybook/builder-vite": "^7.4.2",
		"@storybook/react": "^7.4.2",
		"@storybook/react-vite": "^7.4.2",
		"@storybook/testing-library": "^0.2.1",
		"@testing-library/react": "^14.0.0",
		"@types/lodash.throttle": "^4.1.7",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@types/react-transition-group": "^4.4.6",
		"@types/uuid": "^9.0.4",
		"@types/w3c-image-capture": "^1.0.8",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@vitejs/plugin-react": "^4.0.3",
		"cross-env": "^7.0.3",
		"dotenv": "^16.3.1",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"eslint-plugin-storybook": "^0.6.14",
		"gvegas-cfc": "^3.0.9",
		"jsdom": "^22.1.0",
		"loki-sb7": "^0.33.0",
		"postcss": "^8.4.31",
		"prettier": "3.0.3",
		"react-docgen-typescript": "^2.2.2",
		"reg-cli": "^0.17.7",
		"rollup-plugin-visualizer": "^5.9.2",
		"sass": "^1.67.0",
		"storybook": "^7.4.2",
		"typescript": "^5.0.2",
		"vite": "^4.4.5",
		"vite-plugin-static-copy": "^0.17.0",
		"vite-plugin-svgr": "^3.2.0",
		"vitest": "^0.34.4"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
